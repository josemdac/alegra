<script> 

loadForm('form_01');
function storeContact(){

	lang = "<?php echo $_GET['lang']; ?>"
	if(!lang){
		lang = 'es';
		}
	data = btoa(JSON.stringify(collectValues()))
	url = "<?= $this->url('alegra', ['action' => 'store',]) ?>"+'?data='+data+'&lang='+lang

	window.location.href = url;
	
	}

</script>

